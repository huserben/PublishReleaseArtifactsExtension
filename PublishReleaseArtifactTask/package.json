  "dependencies": {
    "azure-devops-node-api": "^10.0.0",
    "azure-pipelines-task-lib": "^2.9.3",
    "zip-local": "^0.3.5"
  },
  "devDependencies": {
    "@types/mocha": "^7.0.1",
    "@types/node": "^13.7.0",
    "@types/q": "^1.5.2",
