  "license": "MIT",
  "bugs": {
    "url": "https://github.com/huserben/PublishReleaseArtifactsExtension/issues"
  },
  "homepage": "https://github.com/huserben/PublishReleaseArtifactsExtension#readme",
  "dependencies": {
    "azure-devops-node-api": "^14.1.0",
    "azure-pipelines-task-lib": "^4.17.3",
    "zip-local": "^0.3.5"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.9",
    "@types/node": "^22.8.6",
    "@types/q": "^1.5.8",
    "mocha": "^10.8.2",
    "sync-request": "^6.1.0",
    "typescript": "^5.6.3"
  }
}
