{
   "manifestVersion": 1,
   "id": "ReleaseArtifacts",
   "name": "Publish and Download Artifacts from Release Pipelines",
   "version": "1.1.0",
   "publisher": "benjhuser",
   "targets": [
      {
         "id": "Microsoft.VisualStudio.Services"
      }
   ],
   "description": "Tasks for uploading and downloading artifacts in a release pipeline",
   "categories": [
      "Azure Pipelines"
   ],
   "icons": {
      "default": "images/task_icon.png"
   },
   "content":{
      "details":{
			"path": "overview.md"
		},
		"license": {
			"path": "LICENSE"
		}
   },
   "screenshots": [
		{
			"path": "images/task_selection.png"
		}
   ],
   "links":{
		"repository":{
			"uri": "https://github.com/huserben/PublishReleaseArtifactsExtension"
		},
		"license":{
			"uri": "https://github.com/huserben/PublishReleaseArtifactsExtension/blob/master/LICENSE"
		}
	},
	"repository": {
		"type": "git",
		"uri":	"https://github.com/huserben/PublishReleaseArtifactsExtension"
	},
   "files": [
      {
         "path": "PublishReleaseArtifactTask"
      },
      {
         "path": "DownloadReleaseArtifactTask"
      }
   ],
   "contributions": [
      {
         "id": "PublishReleaseArtifact",
         "type": "ms.vss-distributed-task.task",
         "targets": [
            "ms.vss-distributed-task.tasks"
         ],
         "properties": {
            "name": "PublishReleaseArtifactTask"
         }
      },
      {
         "id": "DownloadReleaseArtifact",
         "type": "ms.vss-distributed-task.task",
         "targets": [
            "ms.vss-distributed-task.tasks"
         ],
         "properties": {
            "name": "DownloadReleaseArtifactTask"
         }
      }
   ],
   "branding": {
      "color": "rgb(255, 153, 0)",
      "theme": "light"
  },
  "galleryFlags": [
      "Private"
  ]
}